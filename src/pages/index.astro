---
import Layout from '../layouts/Default.astro'
import Hero from '../components/Hero.svelte'
import TechStackItem from '../components/TechStackItem.svelte'
import SocialIconItem from '../components/SocialIconItem.svelte'
import Perspective from '../components/Perspective.svelte'
import Calendly from '../components/Calendly.svelte'

---

<Layout>
  <div>
    <Hero client:load
      title='
      <span class="font-black tracking-tight text-transparent text-stroke">your partner</span>
      <br/>
      <span class="font-light">—</span>
      <span class="font-black text-transparent text-stroke">in</span>
      <span class="font-black">design</span>
      <span class="font-black text-transparent text-stroke"> & </span>
      <br/>
      <span class="font-black">development</span>'
      desc='<span class="flex flex-col gap-1 sm:flex-row">
        <span>
          We create <span class="font-bold">apps</span> and <span class="font-bold">websites</span>
        </span>
        <span>
          your clients will 
          <svg class="inline-block w-6 h-6 fill-current text-primary" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><title>love</title><path d="M256,448a32,32,0,0,1-18-5.57c-78.59-53.35-112.62-89.93-131.39-112.8-40-48.75-59.15-98.8-58.61-153C48.63,114.52,98.46,64,159.08,64c44.08,0,74.61,24.83,92.39,45.51a6,6,0,0,0,9.06,0C278.31,88.81,308.84,64,352.92,64,413.54,64,463.37,114.52,464,176.64c.54,54.21-18.63,104.26-58.61,153-18.77,22.87-52.8,59.45-131.39,112.8A32,32,0,0,1,256,448Z"/></svg>
        </span>
      </span>'
    >
      <a class="tracking-wide btn btn-primary btn-lg btn-wide" href="#contact">Let's Get Started!</a>
    </Hero>
  </div>


  <div class="flex flex-col items-center">
    <div class="w-full max-w-6xl">
      <div class="px-6 pt-20">
        <h2 class="text-7xl md:text-[10vw] xl:text-[10rem] font-title font-black text-transparent text-stroke leading-[0.7em] mb-4">
          What we're offering
        </h2>
        <p class="py-2 pl-5 mb-5 ml-5 border-l-2 border-opacity-50 opacity-50 border-base-content">
          We help you start your business with a modern, <br/>user-friendly, and intuitive web application.
        </p>
      </div>
    </div>
  </div>
  <div class="flex w-full px-6 overflow-x-auto md:justify-center">
    <div class="flex max-w-6xl gap-10 py-20 flex-nowrap md:grid md:grid-cols-2">

      <Perspective scale={8} client:load>
        <div class="grid w-64 h-64 bg-base-200 rounded-box place-items-center md:w-auto">
          <div class="flex flex-col items-center gap-4 p-10">
            <svg class="w-12 h-12 text-pink-500 fill-current" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M22.61,7.05,17,1.39a1,1,0,0,0-.71-.29,1,1,0,0,0-.7.29L1.39,15.54a1,1,0,0,0,0,1.41l5.66,5.66a1,1,0,0,0,.71.29,1,1,0,0,0,.7-.29l2.83-2.83h0l8.49-8.49h0l2.83-2.83A1,1,0,0,0,22.61,7.05ZM19.07,9.17l-.71-.71a1,1,0,0,0-1.41,0,1,1,0,0,0,0,1.42l.71.71L16.24,12,14.12,9.88a1,1,0,0,0-1.41,1.41l2.12,2.12-1.42,1.42-.7-.71a1,1,0,1,0-1.42,1.42l.71.7-1.41,1.42L8.46,15.54a1,1,0,0,0-1.41,0,1,1,0,0,0,0,1.41l2.12,2.12L7.76,20.49,3.51,16.24,16.24,3.51l4.25,4.25Z"/></svg>
            <div class="text-2xl font-bold text-pink-500">Design</div>
            <p class="opacity-50">Product design, UI/UX design, Design systems</p>
          </div>
        </div>
      </Perspective>


      <Perspective scale={8} client:load>
        <div class="grid w-64 h-64 bg-base-200 rounded-box place-items-center md:w-auto">
          <div class="flex flex-col items-center gap-4 p-10">
            <svg class="w-12 h-12 text-yellow-500 fill-current" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M9.71,6.29a1,1,0,0,0-1.42,0l-5,5a1,1,0,0,0,0,1.42l5,5a1,1,0,0,0,1.42,0,1,1,0,0,0,0-1.42L5.41,12l4.3-4.29A1,1,0,0,0,9.71,6.29Zm11,5-5-5a1,1,0,0,0-1.42,1.42L18.59,12l-4.3,4.29a1,1,0,0,0,0,1.42,1,1,0,0,0,1.42,0l5-5A1,1,0,0,0,20.71,11.29Z"/></svg>
            <div class="text-2xl font-bold text-yellow-500">Development</div>
            <p class="opacity-50">Developing scalable web apps and mobile apps</p>
          </div>
        </div>
      </Perspective>


      <Perspective scale={8} client:load>
        <div class="grid w-64 h-64 bg-base-200 rounded-box place-items-center md:w-auto">
          <div class="flex flex-col items-center gap-4 p-10">
            <svg class="w-12 h-12 text-blue-500 fill-current" xmlns="http://www.w3.org/2000/svg" data-name="Layer 1" viewBox="0 0 24 24"><path d="M8,6A1,1,0,1,0,7,5,1,1,0,0,0,8,6ZM21,19H14.82A3,3,0,0,0,13,17.18V15h4a3,3,0,0,0,3-3V10a3,3,0,0,0-.78-2A3,3,0,0,0,20,6V4a3,3,0,0,0-3-3H7A3,3,0,0,0,4,4V6a3,3,0,0,0,.78,2A3,3,0,0,0,4,10v2a3,3,0,0,0,3,3h4v2.18A3,3,0,0,0,9.18,19H3a1,1,0,0,0,0,2H9.18a3,3,0,0,0,5.64,0H21a1,1,0,0,0,0-2ZM6,4A1,1,0,0,1,7,3H17a1,1,0,0,1,1,1V6a1,1,0,0,1-1,1H7A1,1,0,0,1,6,6Zm1,9a1,1,0,0,1-1-1V10A1,1,0,0,1,7,9H17a1,1,0,0,1,1,1v2a1,1,0,0,1-1,1Zm5,8a1,1,0,1,1,1-1A1,1,0,0,1,12,21ZM8,10a1,1,0,1,0,1,1A1,1,0,0,0,8,10Z"/></svg>
            <div class="text-2xl font-bold text-blue-500">Deployment</div>
            <p class="opacity-50">Automated deployment workflow and CI/CD pipelines</p>
          </div>
        </div>
      </Perspective>


      <Perspective scale={8} client:load>
        <div class="grid w-64 h-64 bg-base-200 rounded-box place-items-center md:w-auto">
          <div class="flex flex-col items-center gap-4 p-10">
            <svg class="w-12 h-12 text-green-500 fill-current" xmlns="http://www.w3.org/2000/svg" data-name="Layer 1" viewBox="0 0 24 24"><path d="M21.71,15.58l-4.52-4.51a6.85,6.85,0,0,0,.14-1.4A7.67,7.67,0,0,0,6.42,2.72a1,1,0,0,0-.57.74,1,1,0,0,0,.28.88l4.35,4.34-1.8,1.8L4.34,6.13a1,1,0,0,0-.88-.27,1,1,0,0,0-.74.56,7.67,7.67,0,0,0,7,10.91,6.85,6.85,0,0,0,1.4-.14l4.51,4.52a1,1,0,0,0,1.42,0,1,1,0,0,0,0-1.42l-4.9-4.9a1,1,0,0,0-.95-.26,5.88,5.88,0,0,1-1.48.2A5.67,5.67,0,0,1,4,9.67a6,6,0,0,1,.08-1L8,12.6a1,1,0,0,0,1.42,0L12.6,9.39A1,1,0,0,0,12.6,8L8.71,4.08a6.12,6.12,0,0,1,1-.08,5.67,5.67,0,0,1,5.66,5.67,5.88,5.88,0,0,1-.2,1.48,1,1,0,0,0,.26.95l4.9,4.9a1,1,0,0,0,1.42-1.42Z"/></svg>
            <div class="text-2xl font-bold text-green-500">Maintainance</div>
            <p class="opacity-50">Continuous monitoring, maintenance and support</p>
          </div>
        </div>
      </Perspective>


    </div>
  </div>



