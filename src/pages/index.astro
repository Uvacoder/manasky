                <SocialIconItem title="Twitter" slug="twitter" rel="nofollow" url="https://twitter.com/saadeghi"/>
                <SocialIconItem title="GitHub" slug="github" rel="nofollow" url="https://github.com/saadeghi"/>
                <SocialIconItem title="Atbox resume" slug="atbox" rel="nofollow" url="https://atbox.io/pouya"/>
              </div>

              <h4 class="mt-8 mb-4 font-bold">Favorite tools</h4>
              <div class="flex flex-wrap gap-4">
                <TechStackItem title="Sketch" slug="sketch" />
                <TechStackItem title="HTML" slug="html" />
